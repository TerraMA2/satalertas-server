{
  "name": "satalertas-server",
  "version": "2.0.0-dev",
  "private": true,
  "scripts": {
    "start": "DEBUG=satalertas-server:development NODE_ENV=development nodemon --inspect ./bin/www",
    "dev": "DEBUG=satalertas-server:development NODE_ENV=development nodemon --inspect ./bin/www",
